{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "ES6",
    "allowJs": true,
    "lib": [
      "es6",
      "dom"
    ],
    "typeRoots": [],
    "types": [],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
